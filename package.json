{
  "name": "dmr-extension-w3c",
  "version": "1.2.1",
  "description": "Digital Market Research by INFOnline W3C extension",
  "main": "index.js",
  "repository": "git@vgitbn003.infon:IAM/dmr-extension-w3c.git",
  "author": "Jens Oswald <joswald@infonline.de>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build:chrome": "node ./build/build.js --env production",
    "build:edge": "node ./build/build.js --env production --vendor edge",
    "build:firefox": "node ./build/build.js --env production --vendor firefox",
    "build:opera": "node ./build/build.js --env production --vendor opera",
    "build:safari": "node ./build/build.js --env production --vendor safari",
    "bump:major": "bump -M -y package.json src/w3c/manifest.json",
    "bump:minor": "bump -m -y package.json src/w3c/manifest.json",
    "bump:patch": "bump -p -y package.json src/w3c/manifest.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "create:licenses": "./node_modules/.bin/npm-license-crawler --onlyDirectDependencies --json ./build/assets/shared/licenses.json",
    "dev": "node ./build/build.js",
    "dev:watch": "node ./build/build.js --watch",
    "pack:chrome": "node ./build/build.js --env production --pack",
    "pack:edge": "node ./build/build.js --env production --vendor edge --pack",
    "pack:firefox": "node ./build/build.js --env production --vendor firefox --pack",
    "pack:opera": "node ./build/build.js --env production --vendor opera --pack",
    "postinstall": "yarn run create:licenses",
    "postuninstall": "yarn run create:licenses"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/helpers": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "archiver": "^3.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "bump-cli": "^1.1.3",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "1.1.12",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "mkdirp": "^0.5.1",
    "npm-license-crawler": "^0.1.9",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-safe-parser": "^4.0.1",
    "style-loader": "^0.23.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.0.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-merge": "^4.1.4",
    "yargs": "^12.0.1",
    "zip-webpack-plugin": "^3.0.0"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 edge versions",
    "last 10 opera versions",
    "last 10 firefox versions",
    "last 10 safari versions"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "ua-parser-js": "^0.7.18",
    "vue": "^2.6.10",
    "vue-i18n": "^8.9.0",
    "vue-router": "^3.0.2",
    "vuetify": "^1.5.7",
    "vuex": "^3.1.0"
  }
}
